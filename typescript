// Total weight of all completed pickups
const totalWeight = pickups?.reduce((sum, p) => sum + (p.actualWeight || 0), 0) || 0;

// CO2 Saved: 2.5 kg CO2 per kg of material recycled
const co2Saved = Math.round(totalWeight * 2.5);

// Water Conserved: 17 liters per kg of material recycled
const waterConserved = Math.round(totalWeight * 17);
